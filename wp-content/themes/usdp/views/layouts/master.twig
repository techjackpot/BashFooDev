<head>
	{% include 'partials/head.twig' %}
	{% block head %}
	{% endblock %}
</head>
<body class="{{ body_class }} {{ post.slug }}">

	<!-- START PAGE HEADER -->
	{% block header %}
		{% include 'partials/header.twig' %}
	{% endblock %}
	<!-- END PAGE HEADER -->

	<!-- START MAIN CONTENT -->
	<section id="mobile-wrap">
		<main role="main" class="page-content">
			{% block content %}
			{% endblock %}
		</main>
		<!-- END MAIN CONTENT -->

		<!-- START PAGE FOOTER -->
		{% block footer %}
			{% include 'partials/footer.twig' %}
		{% endblock %}
	</section><!--/#mobile-wrap-->
	{{ fn('wp_footer') }}
	<!-- END PAGE FOOTER -->

	<!-- AFTER SCRIPTS -->
	{% block after_scripts %}
	{% endblock %}

	{% if constant('ENV') == 'local' or constant('ENV') == 'stg' %}
		{# {% include 'debug/scope.twig' %} #}
	{% elseif constant('ENV') == 'prd' %}

	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
